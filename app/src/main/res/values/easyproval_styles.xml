<resources>

    <!-- Base application theme. -->
    <style name="easyproval_AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/easyaproval_colorPrimary</item>
        <item name="colorPrimaryDark">@color/easyaproval_colorPrimaryDark</item>
        <item name="colorAccent">@color/easyaproval_colorAccent</item>
    </style>



    <style name="easyproval_apply_left_title_name_field">

    <item name="android:layout_width">@dimen/easyaproavl_width02</item>

    <item name="android:layout_height">wrap_content</item>

    <item name="android:textColor">#666666</item>

    <item name="android:textSize">@dimen/easyaproavl_textSize01</item>

</style>


    <style name="easyproval_apply_name_field">

    <item name="android:layout_width">wrap_content</item>

    <item name="android:layout_height">wrap_content</item>

    <item name="android:textColor">#16ADFE</item>

    <item name="android:textSize">@dimen/easyaproavl_textSize01</item>

</style>


    <style name="easyproval_divide_line">

    <item name="android:layout_width">match_parent</item>

    <item name="android:layout_height">1dp</item>

    <item name="android:background">#DDDDDD</item>

</style>


    <style name="easyaproval_divide_image">

    <item name="android:layout_width">match_parent</item>

    <item name="android:layout_height">1px</item>

    <item name="android:background">@drawable/easy_line_dot</item>

</style>


    <style name="easyproval_button_text">

    <item name="android:textColor">#666666</item>

    <item name="android:textSize">@dimen/easyaproavl_textSize02</item>

    <item name="android:gravity">center</item>

    <item name="android:layout_margin">3dp</item>

    <item name="android:background">@drawable/login_id_edit_box</item>

</style>


   <style name="easyaproval_editview_02">

    <item name="android:layout_width">match_parent</item>

    <item name="android:layout_height">wrap_content</item>

    <item name="android:background">@drawable/login_id_edit_box</item>

    <item name="android:textColor">#666666</item>

    <item name="android:singleLine">true</item>

    <item name="android:layout_gravity">center_vertical</item>

    <item name="android:padding">5dp</item>

</style>


    <style name="easyaproval_radio_button">

    <item name="android:layout_width">wrap_content</item>

    <item name="android:layout_height">wrap_content</item>

    <item name="android:background">@drawable/easy_radio_off</item>

</style>


    <style name="easyaproval_radio_text">

    <item name="android:layout_width">wrap_content</item>

    <item name="android:layout_height">wrap_content</item>

    <item name="android:textColor">#666666</item>

    <item name="android:textSize">@dimen/easyaproavl_textSize02</item>

    <item name="android:paddingLeft">5px</item>

    <item name="android:clickable">true</item>

</style>


    <style name="easyaproavl_toolbar_button">

    <item name="android:layout_width">@dimen/easyaproavl_width01</item>

    <item name="android:layout_height">@dimen/easyaproavl_height01</item>

    <item name="android:background">@drawable/easy_common_selector_toolbar</item>

    <item name="android:gravity">center</item>

    <item name="android:textStyle">bold</item>

    <item name="android:textColor">#FFFFFF</item>

    <item name="android:textSize">24px</item>

</style>

